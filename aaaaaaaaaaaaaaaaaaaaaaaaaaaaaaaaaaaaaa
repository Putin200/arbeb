import pygame
import requests


def ll(x, y):
    return '{0},{1}'.format(x, y)

def MapPar(self):
    def __init__(self):
        self.lat = 32.676199
        self.lon = -117.157694
        self.zoom = 17
        self.type = 'map'
    def ll(self):
        return ll(self.lon, self.lat)

def zag_map(mp):
    map_req = "http://static-maps.yandex.ru/1.x/?ll={ll}&z={z}&l={type}".format(ll=mp.ll(),z=mp.zoom(),type=mp.type())

    response = requests.get(map_req)

